<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="Domain.Participant" table="Participanti">
        <id name="id" column="ID">
            <generator class="increment"/>
        </id>
        <property name="nume" type="java.lang.String" column="Nume"/>
        <property name="varsta" type="java.lang.Integer" column="Varsta"/>
        <property name="nrParticipari" type="java.lang.Integer" column="NumarParticipari"/>
    </class>

</hibernate-mapping>
